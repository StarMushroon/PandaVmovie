        <activity android:name=".MainActivity">
        </activity>
        <activity android:name=".ui.splash.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".ui.splash.GuideActivity">
        </activity>
        <activity android:name=".ui.search.SearchActivity">
        </activity>
        <activity android:name=".ui.series.detail.SeriesDetailActivity">
        </activity>
        <activity android:name=".ui.home.home_Newest.detail.NewestDetailActivity">
        </activity>
        <activity android:name=".ui.home.home_Channel.detail.ChannelListActivity">
        </activity>
        <activity android:name=".ui.behind.behindContent.BehindContentWebActivity">
        </activity>
        <activity android:name=".ui.login.LoginActivity">
        </activity>
        <activity android:name=".ui.home.coverExtra.setting.SettingActivity">
        </activity>
        <activity android:name=".ui.home.coverExtra.message.MessageActivity">
        </activity>
        <activity android:name=".ui.home.coverExtra.cache.DownloadActivity">
        </activity>
        <activity android:name=".ui.home.coverExtra.like.LikeActivity">
